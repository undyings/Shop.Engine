<?xml version="1.0"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5" requestValidationMode="2.0" />
    <customErrors mode="Off"/>
    <pages validateRequest="false" controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.webServer>
    <handlers>
      <add name="gisLoader" verb="*" path="gis.html" type="Shop.Engine.GisLoader"/>
      <add name="syncJsFile" verb="*" path="sync.js" type="NitroBolt.Wui.SyncScriptHandler"/>
      <add name="htmlFiles" verb="*" path="*.html" type="Shop.Engine.HSync"/>
      <add name="defaultAspxFiles" verb="*" path="default.aspx" type="Shop.Engine.HSync"/>
      <add name="jsFiles" verb="*" path="*.html.js" type="Shop.Engine.HSync"/>
      <add name="woffFiles" verb="*" path="*.woff" type="System.Web.StaticFileHandler"/>
      <add name="woff2Files" verb="*" path="*.woff2" type="System.Web.StaticFileHandler"/>      
      <add name="tileUploader" verb="*" path="tileupload.js" type="Shop.Engine.TileUploader"/>
      <add name="tileLargeUploader" verb="*" path="tilelargeupload.js" type="Shop.Engine.TileLargeUploader"/>
      <add name="filesUploader" verb="*" path="filesupload.js" type="Shop.Engine.FilesUploader"/>
    </handlers>
    <defaultDocument>
        <files>
            <remove value="Index.html" />
            <add value="index.html" />
        </files>
    </defaultDocument>  
  </system.webServer>
</configuration>